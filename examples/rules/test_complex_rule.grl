rule "Complex Business Rule" salience 10 {
  when
    (((user.vipStatus == true) && (order.amount > 500)) || ((date.isHoliday == true) && (order.hasCoupon == true)))
  then
    apply_discount(20000);
}

rule "Simple Backup Rule" salience 5 {
  when
    (user.isGuest == false)
  then
    set(user.hasAccess, true);
}
